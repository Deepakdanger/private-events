<h1>Users#show</h1>

<h3>My Created Events</h3>
<ul>
<% @created_events.each do |event| %>
    <li>
    <%=event.event_name%><br>
    </li>
<%end%>
</ul>

<h3>My Up-coming Attending Events</h3>
<ul>
<% @attending_events.future_attend.each do |event| %>
    <li>
    <%=event.event_name%><br>
    </li>
<%end%>
</ul>

<h3>My Past Attended Events</h3>
<ul>
<% @attending_events.past_attend.each do |event| %>
    <li>
    <%=event.event_name%><br>
    </li>
<%end%>
</ul>

